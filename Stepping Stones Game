#include <bits/stdc++.h>

using namespace std;
typedef long long ll;
int main()
{
    int t;
     cin>>t;
     while(t--){
        ll n;
        cin>>n;
        ll root=(sqrt(8*n+1)-1)/2;
       if((root*(root+1))/2==n)
            cout<<"Go On Bob"<<" "<<root<<endl;
        else
            cout<<"Better Luck Next Time"<<endl;

     }



    return 0;
}
