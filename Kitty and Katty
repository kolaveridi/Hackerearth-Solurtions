using namespace std;
typedef long long ll;

int main(){
    int T;
    cin >> T;
    for(int a0 = 0; a0 < T; a0++){
        ll n;
        cin >> n;
        if(n==1)
            cout<<"Kitty"<<endl;
        if(n>1){
        if(n%2==0)
            cout<<"Kitty"<<endl;
        else
            cout<<"Katty"<<endl;
    }
    }
    return 0;
}
